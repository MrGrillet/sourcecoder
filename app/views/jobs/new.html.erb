<h1>Create a new job listing</h1>

<%= form_for @job do |f| %>
<p>
	<%= f.label :title %><br>
	<%= f.text_field :title %>
</p>
<p>
	<%= f.label :description %><br>
	<%= f.text_area :description %>
</p>
<p>
	<%= f.label :location %><br>
	<%= f.text_field :location %>
</p>
<p>
	<%= f.submit %>
	
</p>
<% end %>